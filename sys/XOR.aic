## Input        : REGISTFD, REGISTFE
## Output       : REGISTFD
## Registers    : REGIST80, REGIST81, REGIST82, REGIST83
## Dependencies : ORR

# (⌐FD ∧ FE) ∨ (FD ∧ ⌐FE)

# Work copies
MOV REGIST80 REGISTFD
MOV REGIST81 REGISTFE
MOV REGIST82 REGISTFD
MOV REGIST83 REGISTFE

# ⌐FD
NOT REGIST80
MOV REGIST80 REGISTFD

# ⌐FE
NOT REGIST83
MOV REGIST83 REGISTFD

# ⌐FD ∧ FE
AND REGIST80 REGIST81
MOV REGIST80 REGISTFD

# FD ∧ ⌐FE
AND REGIST82 REGIST83
MOV REGIST82 REGISTFD

# ∨
ORR REGIST80 REGIST82
